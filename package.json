    "vue-chemistry": "^0.2.2",
    "vue-router": "^4.1.5",
    "vuefire": "^3.0.0-alpha.11",
    "vuetify": "^3.0.1"
  },
  "devDependencies": {
    "@mdi/js": "6.2.95",
    "@vitejs/plugin-vue": "^3.1.0",
    "firebase": "^9.14.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.22",
    "sass": "^1.56.1",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-vue-components": "^0.22.7",
    "unplugin-vue-macros": "^0.11.2",
